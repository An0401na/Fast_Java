package ch14;

public class Taxi {
	String taxiName;
	int money;
	int distance;
	
	public Taxi (String taixName) {
		this.taxiName=taixName;
	}
	
	public void take(int money, int distance) {
		this.money+=money;
		this.distance+=distance;
	}
	
	public void showTaxiInfo() {
		System.out.println(taxiName+"회사 택시의 주행거리는 " + distance+"이고, 수입은 "+ money+"원 입니다.");
	}
	
	
	
}
